{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "include": [
    "src"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "vite/client"
    ],
    "paths": {
      "@*": [
        "src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  }
}
